{
  "name": "marketplace-protocol",
  "version": "1.0.0",
  "description": "Decentralized marketplace protocol",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "dev": "nodemon src/index.ts",
    "dev:api": "tsx watch --env-file=.env src/api/server.ts",
    "start:api": "tsx --env-file=.env src/api/server.ts",
    "build": "tsc",
    "lint": "eslint src --ext .ts",
    "test:layer0": "ts-node scripts/test-layer0.ts",
    "test:layer1": "ts-node scripts/test-layer1.ts",
    "test:layer2": "ts-node scripts/test-layer2.ts",
    "test:layer3": "ts-node scripts/test-layer3.ts",
    "test:layer4": "ts-node scripts/test-layer4.ts",
    "test:layer5": "ts-node scripts/test-layer5.ts",
    "test:layer6": "ts-node scripts/test-layer6.ts",
    "test:identity-routes": "ts-node scripts/test-identity-routes.ts",
    "test:catalog-routes": "ts-node scripts/test-catalog-routes.ts",
    "test:order-routes": "tsx --env-file=.env scripts/test-order-routes.ts",
    "test:logistics-routes": "tsx --env-file=.env scripts/test-logistics-routes.ts",
    "test:trust-routes": "tsx --env-file=.env scripts/test-trust-routes.ts",
    "test:all": "npm run test:layer0 && npm run test:layer1 && npm run test:layer2 && npm run test:layer3 && npm run test:layer4 && npm run test:layer5 && npm run test:layer6"
  },
  "keywords": [
    "marketplace",
    "protocol",
    "decentralized",
    "bitcoin",
    "web5"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.39.0",
    "compression": "^1.8.1",
    "cors": "^2.8.5",
    "crypto": "^1.0.1",
    "dotenv": "^17.2.3",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.0",
    "express-validator": "^7.3.0",
    "helmet": "^7.2.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.1",
    "uuid": "^9.0.1",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "@types/compression": "^1.8.1",
    "@types/cors": "^2.8.19",
    "@types/express": "^4.17.25",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^20.19.24",
    "@types/supertest": "^6.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "axios": "^1.13.1",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.10",
    "supertest": "^7.1.4",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
