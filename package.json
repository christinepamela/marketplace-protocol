{
  "name": "marketplace-protocol",
  "version": "1.0.0",
  "description": "Decentralized protocol for globalizing small businesses",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "dev": "nodemon --exec ts-node src/index.ts",
    "test": "jest",
    "test:layer0": "ts-node scripts/test-layer0.ts",
    "test:layer1": "ts-node scripts/test-layer1.ts",
    "test:layer2": "ts-node scripts/test-layer2.ts",
    "test:layer3": "ts-node scripts/test-layer3.ts",
    "test:layer4": "ts-node scripts/test-layer4.ts",
    "test:layer5": "ts-node scripts/test-layer5.ts",
    "test:layer6": "ts-node scripts/test-layer6.ts",
    "api:dev": "ts-node-dev --respawn --transpile-only src/api/server.ts",
    "api:build": "tsc --project tsconfig.api.json",
    "api:start": "node dist/api/server.js",
    "api:test": "jest --testPathPattern=src/api",
    "generate-keys": "ts-node scripts/generate-keys.ts"
  },
  "keywords": [
    "marketplace",
    "protocol",
    "bitcoin",
    "small-business",
    "global-trade"
  ],
  "author": "Christine Pamela",
  "license": "MIT",
  "dependencies": {
    "@supabase/supabase-js": "^2.75.1",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.5",
    "helmet": "^7.1.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "^1.10.0",
    "uuid": "^9.0.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.5",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.10.6",
    "@types/supertest": "^6.0.2",
    "@types/uuid": "^9.0.8",
    "nodemon": "^3.0.2",
    "supertest": "^6.3.3",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}